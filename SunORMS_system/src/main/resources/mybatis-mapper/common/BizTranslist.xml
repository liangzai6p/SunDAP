<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sunyard.ars.common.dao.BizTranslistDao">
  <update id="updateArthiveData" parameterType="java.util.HashMap" >
  	update BIZ_TRANSLIST SET ARCHIVE_STATE = '2'
   	<where>
   		<if test="siteNo != ''">
    		and LANUCH_BRANCHNO = #{siteNo,jdbcType=VARCHAR}
    	</if>
    	<if test="occurDate != ''">
    		and TRANS_DATE = #{occurDate,jdbcType=DECIMAL}
    	</if>
   	</where>
 </update>
</mapper>

  

  



  
  



	

